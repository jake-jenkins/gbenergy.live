generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model GridLive {
  createdAt DateTime @id @default(now())
  BIOMASS   Int
  CCGT      Int
  COAL      Int
  INTELEC   Int
  INTEW     Int
  INTFR     Int
  INTGRNL   Int
  INTIFA2   Int
  INTIRL    Int
  INTNED    Int
  INTNEM    Int
  INTNSL    Int
  INTVKL    Int
  NPSHYD    Int
  NUCLEAR   Int
  OCGT      Int
  OIL       Int
  OTHER     Int
  PS        Int
  WIND      Int
}
